using System;

namespace challange
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int y = 1; y < 6; y++)
            {
                for (int i = 1; i < 11; i++)
                {
                    Console.WriteLine(i);
                }
                for (int x = 9; x > 1; x--)
                {
                    Console.WriteLine(x);
                }
            }
        }
        }
    }
